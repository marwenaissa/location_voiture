# .env.test

# Symfony Kernel class for the test environment
KERNEL_CLASS='App\Kernel'

# Secret key for the test environment
APP_SECRET='$ecretf0rt3st'

# Symfony deprecation helper, useful for debugging deprecation notices
SYMFONY_DEPRECATIONS_HELPER=999999

# Panther environment (useful for browser testing with Symfony Panther)
PANTHER_APP_ENV=panther

# Directory where Panther screenshots are saved when an error occurs during browser testing
PANTHER_ERROR_SCREENSHOT_DIR=./var/error-screenshots

# Database configuration for the test environment (ensure you have a test database set up)
DATABASE_URL="mysql://root:@127.0.0.1:3306/locationv_test"

# Other environment-specific configurations can be added here
